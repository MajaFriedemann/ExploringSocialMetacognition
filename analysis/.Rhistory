geom_point() +
geom_errorbar() +
facet_grid(variable~., labeller = label_value, scales = "free_y") +
labs(x = "", y = "mean +/- 95% CI") +
theme_light() +
theme(axis.text.x = element_text(angle = 90, hjust = 1))
